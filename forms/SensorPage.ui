   <string notr="true">/* ============================================================
   GLOBAL SETTINGS
   ============================================================ */
QWidget#SensorPage {
    background-color: #f0f2f5;
    font-family: &quot;Microsoft YaHei UI&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 10pt;
    color: #2c3e50;
}

/* ============================================================
   CARD UI (Group Boxes)
   ============================================================ */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #e4e7ed;
    border-radius: 8px;
    margin-top: 12px;
    padding-top: 24px;
    padding-bottom: 12px;
    padding-left: 12px;
    padding-right: 12px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 16px;
    top: 2px;
    padding: 0 6px;
    font-size: 11pt;
    font-weight: bold;
    color: #303133;
    background-color: transparent;
}

/* Hover effect for interactive config cards */
QGroupBox#groupBox_vk701:hover,
QGroupBox#groupBox_mdb:hover,
QGroupBox#groupBox_motor:hover,
QGroupBox#groupBox_control:hover {
    border-color: #c0c4cc;
}

/* ============================================================
   DARK MONITOR PANEL (Special Styling)
   ============================================================ */
QGroupBox#groupBox_status {
    background-color: #21252b;
    border: 1px solid #3e4451;
}

QGroupBox#groupBox_status::title {
    color: #abb2bf;
}

QGroupBox#groupBox_status QLabel {
    color: #dcdfe6;
}

/* Statistics Label */
QLabel#label_statistics {
    font-family: &quot;Consolas&quot;, &quot;Monaco&quot;, monospace;
    color: #abb2bf;
    font-size: 9pt;
    background: #282c34;
    padding: 6px;
    border-radius: 4px;
    border: 1px solid #3e4451;
}

QLabel#label_round_id {
    color: #61afef;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-weight: bold;
}

QLabel#label_status {
    color: #98c379;
    font-weight: bold;
}

/* ============================================================
   INPUT CONTROLS
   ============================================================ */
QLineEdit, QSpinBox, QTextEdit {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 5px 8px;
    background-color: #ffffff;
    color: #606266;
    selection-background-color: #409eff;
}

QLineEdit:focus, QSpinBox:focus, QTextEdit:focus {
    border: 1px solid #409eff;
    background-color: #ecf5ff;
}

QLineEdit:disabled, QSpinBox:disabled {
    background-color: #f5f7fa;
    color: #c0c4cc;
    border-color: #e4e7ed;
}

/* ============================================================
   BUTTONS
   ============================================================ */
QPushButton {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 6px 12px;
    background-color: #ffffff;
    color: #606266;
    font-weight: 500;
    font-size: 10pt;
}

QPushButton:hover {
    color: #409eff;
    border-color: #c6e2ff;
    background-color: #ecf5ff;
}

QPushButton:pressed {
    padding-top: 7px;
    color: #3a8ee6;
    border-color: #3a8ee6;
}

QPushButton:disabled {
    background-color: #f5f7fa;
    color: #c0c4cc;
    border-color: #e4e7ed;
}

/* Connect Buttons */
QPushButton#btn_vk701_connect,
QPushButton#btn_mdb_connect,
QPushButton#btn_motor_connect {
    background-color: #67c23a;
    color: white;
    border: 1px solid #5daf34;
}
QPushButton#btn_vk701_connect:hover,
QPushButton#btn_mdb_connect:hover,
QPushButton#btn_motor_connect:hover {
    background-color: #85ce61;
    border-color: #85ce61;
}

/* Disconnect Buttons */
QPushButton#btn_vk701_disconnect,
QPushButton#btn_mdb_disconnect,
QPushButton#btn_motor_disconnect {
    background-color: #fef0f0;
    color: #f56c6c;
    border: 1px solid #fbc4c4;
}
QPushButton#btn_vk701_disconnect:hover,
QPushButton#btn_mdb_disconnect:hover,
QPushButton#btn_motor_disconnect:hover {
    background-color: #f56c6c;
    color: white;
    border-color: #f56c6c;
}

/* Round Control Buttons */
QPushButton#btn_new_round {
    background-color: #409eff;
    color: white;
    border: none;
}
QPushButton#btn_new_round:hover {
    background-color: #66b1ff;
}

QPushButton#btn_end_round {
    background-color: #f4f4f5;
    color: #909399;
    border: 1px solid #d3d4d6;
}
QPushButton#btn_end_round:hover {
    background-color: #909399;
    color: white;
    border-color: #909399;
}

/* Global Start/Stop */
QPushButton#btn_start_all {
    background-color: #409eff;
    color: white;
    font-size: 12pt;
    font-weight: bold;
    border: none;
    border-radius: 6px;
}
QPushButton#btn_start_all:hover {
    background-color: #66b1ff;
}

QPushButton#btn_stop_all {
    background-color: #f56c6c;
    color: white;
    font-size: 12pt;
    font-weight: bold;
    border: none;
    border-radius: 6px;
}
QPushButton#btn_stop_all:hover {
    background-color: #ff7875;
}

/* ============================================================
   LABELS
   ============================================================ */
QLabel {
    color: #606266;
}

QLabel#label_operator, QLabel#label_note_label {
    font-weight: bold;
    color: #303133;
}
</string>
