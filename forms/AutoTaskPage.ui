   <string notr="true">/* =======================================================
   全局设置
   ======================================================= */
QWidget#AutoTaskPage {
    background-color: #f5f7fa;
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
    color: #333333;
}

/* =======================================================
   分组框 (QGroupBox)
   ======================================================= */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #dcdfe6;
    border-radius: 6px;
    margin-top: 12px;
    padding-top: 15px;
    padding-bottom: 10px;
    padding-left: 10px;
    padding-right: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 10px;
    margin-top: 0px;
    font-size: 15px;
    font-weight: bold;
    color: #2c3e50;
    background-color: transparent;
}

/* =======================================================
   按钮 (QPushButton)
   ======================================================= */
QPushButton {
    border: none;
    border-radius: 4px;
    padding: 6px 15px;
    font-weight: bold;
    font-size: 13px;
    color: #ffffff;
    background-color: #409eff;
}

QPushButton:hover {
    background-color: #66b1ff;
}

QPushButton:pressed {
    background-color: #3a8ee6;
    padding-top: 7px;
    padding-bottom: 5px;
}

QPushButton:disabled {
    background-color: #a0cfff;
    color: #f0f0f0;
}

/* 开始按钮 - 绿色 */
QPushButton#btn_start {
    background-color: #67c23a;
    min-height: 40px;
}
QPushButton#btn_start:hover {
    background-color: #85ce61;
}
QPushButton#btn_start:pressed {
    background-color: #5daf34;
}

/* 暂停按钮 - 橙色 */
QPushButton#btn_pause {
    background-color: #e6a23c;
    min-height: 40px;
}
QPushButton#btn_pause:hover {
    background-color: #ebb563;
}
QPushButton#btn_pause:pressed {
    background-color: #cf9236;
}

/* 恢复按钮 - 蓝色 */
QPushButton#btn_resume {
    background-color: #409eff;
    min-height: 40px;
}

/* 停止按钮 - 灰色 */
QPushButton#btn_stop {
    background-color: #909399;
    min-height: 40px;
}
QPushButton#btn_stop:hover {
    background-color: #a6a9ad;
}
QPushButton#btn_stop:pressed {
    background-color: #82848a;
}

/* 急停按钮 - 红色 */
QPushButton#btn_emergency {
    background-color: #f56c6c;
    font-size: 16px;
    font-weight: 900;
    border-radius: 6px;
    padding: 10px 20px;
    min-width: 150px;
    min-height: 50px;
}
QPushButton#btn_emergency:hover {
    background-color: #f78989;
}
QPushButton#btn_emergency:pressed {
    background-color: #dd6161;
}

/* =======================================================
   进度条 (QProgressBar)
   ======================================================= */
QProgressBar {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    text-align: center;
    background-color: #ebeef5;
    color: #606266;
    font-weight: bold;
}

QProgressBar::chunk {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #409eff, stop:1 #69c0ff);
    border-radius: 3px;
}

/* =======================================================
   列表与表格 (QListWidget, QTableWidget)
   ======================================================= */
QListWidget, QTableWidget {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    background-color: #ffffff;
    selection-background-color: #ecf5ff;
    selection-color: #409eff;
    outline: none;
    gridline-color: #ebeef5;
}

QTableWidget::item {
    padding: 5px;
}

QHeaderView::section {
    background-color: #f5f7fa;
    padding: 6px;
    border: none;
    border-bottom: 1px solid #dcdfe6;
    border-right: 1px solid #ebeef5;
    color: #606266;
    font-weight: bold;
}

/* =======================================================
   日志输出 (QTextBrowser)
   ======================================================= */
QTextBrowser#tb_log {
    background-color: #2c3e50;
    color: #ecf0f1;
    border: 1px solid #34495e;
    border-radius: 4px;
    font-family: &quot;Consolas&quot;, &quot;Monaco&quot;, monospace;
    padding: 5px;
}

/* =======================================================
   标签与状态 (QLabel)
   ======================================================= */
QLabel {
    color: #606266;
}

QLabel#lbl_task_status, QLabel#lbl_current_step {
    background-color: #ecf5ff;
    color: #409eff;
    border: 1px solid #d9ecff;
    border-radius: 4px;
    padding: 4px 8px;
    font-weight: bold;
}

QLabel#lbl_depth, QLabel#lbl_target, QLabel#lbl_elapsed {
    color: #303133;
    font-size: 14px;
}

QLabel#lbl_task_info {
    color: #909399;
    font-style: italic;
}

QLabel#lbl_current_preset {
    color: #606266;
}</string>
