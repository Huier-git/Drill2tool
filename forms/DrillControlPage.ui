   <string notr="true">/* ============================================================
   GLOBAL SETTINGS
   ============================================================ */
QWidget#DrillControlPage {
    background-color: #f0f2f5;
    font-family: &quot;Microsoft YaHei UI&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 10pt;
    color: #2c3e50;
}

/* ============================================================
   TOP BAR ACTIONS
   ============================================================ */
/* Connection Status Label */
QLabel#lbl_connection_status {
    background-color: #909399;
    color: white;
    padding: 6px 12px;
    border-radius: 4px;
    font-weight: bold;
    font-size: 10pt;
}

/* Top Bar Buttons */
QPushButton#btn_connect, QPushButton#btn_init_all,
QPushButton#btn_stop_all, QPushButton#btn_reload_config {
    height: 36px;
    font-size: 10pt;
    padding: 0 20px;
}

QPushButton#btn_connect {
    background-color: #67c23a;
    border: 1px solid #5daf34;
    color: white;
}
QPushButton#btn_connect:hover { background-color: #85ce61; }

QPushButton#btn_init_all {
    background-color: #e6a23c;
    border: 1px solid #dcdfe6;
    color: white;
}
QPushButton#btn_init_all:hover { background-color: #ebb563; }

QPushButton#btn_stop_all {
    background-color: #f56c6c;
    border: 1px solid #f56c6c;
    font-weight: bold;
    color: white;
}
QPushButton#btn_stop_all:hover { background-color: #ff7875; }

/* ============================================================
   TAB WIDGET STYLING
   ============================================================ */
QTabWidget::pane {
    border: 1px solid #e4e7ed;
    border-radius: 4px;
    background: #f0f2f5;
    top: -1px;
}

QTabBar::tab {
    background: #e4e7ed;
    color: #606266;
    padding: 8px 24px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    font-weight: 600;
    min-width: 80px;
}

QTabBar::tab:selected {
    background: #f0f2f5;
    color: #409eff;
    border-top: 3px solid #409eff;
}

QTabBar::tab:hover {
    background: #ffffff;
    color: #409eff;
}

/* ============================================================
   CARD UI (Group Boxes)
   ============================================================ */
QGroupBox {
    background-color: #ffffff;
    border: 1px solid #e4e7ed;
    border-radius: 8px;
    margin-top: 12px;
    padding-top: 24px;
    padding-bottom: 12px;
    padding-left: 12px;
    padding-right: 12px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 16px;
    top: 2px;
    padding: 0 6px;
    font-size: 11pt;
    font-weight: bold;
    color: #303133;
    background-color: transparent;
}

QGroupBox:hover {
    border-color: #c0c4cc;
}

/* ============================================================
   SCROLL AREA
   ============================================================ */
QScrollArea {
    border: none;
    background: transparent;
}
QScrollArea &gt; QWidget &gt; QWidget {
    background: transparent;
}

/* ============================================================
   INPUT CONTROLS
   ============================================================ */
QLineEdit, QSpinBox, QDoubleSpinBox, QComboBox {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 5px 8px;
    background-color: #ffffff;
    color: #606266;
    min-height: 24px;
    selection-background-color: #409eff;
}

QLineEdit:focus, QSpinBox:focus, QDoubleSpinBox:focus, QComboBox:focus {
    border: 1px solid #409eff;
    background-color: #ecf5ff;
}

QLineEdit[readOnly=&quot;true&quot;] {
    background-color: #f5f7fa;
    color: #909399;
}

/* ============================================================
   BUTTONS (General)
   ============================================================ */
QPushButton {
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 6px 12px;
    background-color: #ffffff;
    color: #606266;
    font-weight: 500;
    min-height: 28px;
}

QPushButton:hover {
    color: #409eff;
    border-color: #c6e2ff;
    background-color: #ecf5ff;
}

QPushButton:pressed {
    color: #3a8ee6;
    border-color: #3a8ee6;
    background-color: #ecf5ff;
    padding-top: 7px;
}

/* Primary Action */
QPushButton#btn_Fz_move, QPushButton#btn_Pr_start, QPushButton#btn_Pi_start,
QPushButton#btn_Me_move, QPushButton#btn_Mr_move,
QPushButton#btn_apply_config, QPushButton#btn_save_config {
    background-color: #409eff;
    color: white;
    border: none;
}
QPushButton#btn_Fz_move:hover, QPushButton#btn_Pr_start:hover, QPushButton#btn_Pi_start:hover,
QPushButton#btn_Me_move:hover, QPushButton#btn_Mr_move:hover,
QPushButton#btn_apply_config:hover, QPushButton#btn_save_config:hover {
    background-color: #66b1ff;
}

/* Danger Action */
QPushButton#btn_Fz_stop, QPushButton#btn_Pr_stop, QPushButton#btn_Pi_stop {
    background-color: #fef0f0;
    color: #f56c6c;
    border-color: #fbc4c4;
}
QPushButton#btn_Fz_stop:hover, QPushButton#btn_Pr_stop:hover, QPushButton#btn_Pi_stop:hover {
    background-color: #f56c6c;
    color: white;
    border-color: #f56c6c;
}

/* Initialization */
QPushButton#btn_Fz_init, QPushButton#btn_Pr_init, QPushButton#btn_Pi_unlock,
QPushButton#btn_Cb_init, QPushButton#btn_Sr_init, QPushButton#btn_Dh_connect,
QPushButton#btn_Me_init, QPushButton#btn_Mg_init, QPushButton#btn_Mr_init,
QPushButton#btn_reset_config {
    background-color: #fdf6ec;
    color: #e6a23c;
    border-color: #f5dab1;
}
QPushButton#btn_Fz_init:hover, QPushButton#btn_Pr_init:hover, QPushButton#btn_Pi_unlock:hover,
QPushButton#btn_Cb_init:hover, QPushButton#btn_Sr_init:hover, QPushButton#btn_Dh_connect:hover,
QPushButton#btn_Me_init:hover, QPushButton#btn_Mg_init:hover, QPushButton#btn_Mr_init:hover,
QPushButton#btn_reset_config:hover {
    background-color: #e6a23c;
    color: white;
    border-color: #e6a23c;
}

/* ============================================================
   DATA VISUALIZATION LABELS
