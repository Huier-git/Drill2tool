  <property name="styleSheet">
   <string notr="true">/* 全局背景与字体 */
QWidget#MotorPage {
    background-color: #f5f7fa; /* 浅灰背景，护眼且现代 */
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
    color: #333333;
}

QScrollArea {
    border: none;
    background-color: transparent;
}
QScrollArea &gt; QWidget &gt; QWidget {
    background-color: transparent;
}

/* 电机卡片容器 */
QGroupBox {
    background-color: #ffffff; /* 纯白卡片背景 */
    border: 1px solid #e4e7ed; /* 极淡的边框 */
    border-radius: 8px;        /* 圆角 */
    padding-top: 5px;
}

/* 标题样式 (电机编号) */
QLabel[class=&quot;MotorTitle&quot;] {
    color: #2c3e50;            /* 深蓝灰色标题 */
    font-size: 18px;           /* 字体放大 */
    font-weight: bold;
    border-bottom: 2px solid #409eff; /* 底部蓝条装饰 */
    padding-bottom: 8px;
    margin-bottom: 5px;
}

/* 参数标签样式 (位置、速度等) */
QLabel[class=&quot;ParamLabel&quot;] {
    color: #606266;            /* 次级文字颜色 */
    font-size: 15px;           /* 参数文字放大 */
    font-weight: bold;
}

/* LCD 数字显示屏 - 浅色风格 */
QLCDNumber {
    background-color: #f0f2f5; /* 极浅灰底色 */
    color: #303133;            /* 深色数字，高对比度 */
    border: 1px solid #dcdfe6;
    border-radius: 4px;
}

/* 按钮通用样式 */
QPushButton {
    border: none;
    border-radius: 4px;
    padding: 8px 20px;
    font-size: 14px;
    font-weight: bold;
    color: white;
}

/* 开始按钮 - 蓝色 */
QPushButton#btn_start {
    background-color: #409eff;
}
QPushButton#btn_start:hover {
    background-color: #66b1ff;
}
QPushButton#btn_start:pressed {
    background-color: #3a8ee6;
}

/* 停止按钮 - 红色 */
QPushButton#btn_stop {
    background-color: #f56c6c;
}
QPushButton#btn_stop:hover {
    background-color: #f78989;
}
QPushButton#btn_stop:pressed {
    background-color: #dd6161;
}

/* 状态标签 */
QLabel#label_status {
    color: #e6a23c; /* 橙色状态 */
    font-size: 16px;
    font-weight: bold;
    padding: 5px;
    border: 1px dashed #e6a23c;
    border-radius: 4px;
}</string>
